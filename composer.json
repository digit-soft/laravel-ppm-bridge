{
    "name": "digit-soft/laravel-ppm-bridge",
    "version": "0.2.0",
    "description": "Laravel bridge for PHP-PM.",
    "keywords": ["laravel bridge", "php-pm", "laravel ppm"],
    "license": "MIT",
    "authors": [
        {
            "name": "Digit",
            "email": "digit.vova@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "php-pm/httpkernel-adapter": "*"
    },
    "autoload": {
        "psr-4": {
            "DigitSoft\\LaravelPpm\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
